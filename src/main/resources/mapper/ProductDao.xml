<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.lx.dao.ProductDao">
    <insert id="insertProduct" parameterType="com.cn.lx.entity.Product"
            useGeneratedKeys="true" keyProperty="productId" keyColumn="product_id">
        <!--useGeneratedKeys 成功是 -->
        INSERT INTO
        tb_product(product_name,product_desc,img_addr,
		normal_price,promotion_price,priority,create_time,
		last_edit_time,enable_status,product_category_id,
		shop_id)
		VALUES
		(#{productName},#{productDesc},#{imgAddr},#{normalPrice},#{promotionPrice},#{priority},#{createTime},
		#{lastEditTime},#{enableStatus},#{productCategory.productCategoryId},#{shop.shopId})
    </insert>
</mapper>